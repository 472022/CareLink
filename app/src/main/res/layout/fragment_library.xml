<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F5FA"
    tools:context=".fragment_library">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textView14"
            android:layout_width="401dp"
            android:layout_height="38dp"
            android:gravity="center"
            android:layout_marginTop="5dp"
            android:drawableStart="@drawable/ic_baseline_arrow_back_24"
            android:paddingLeft="15dp"
            android:paddingRight="20dp"
            android:text=""
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:layout_marginTop="5dp"
            android:paddingLeft="19dp"
            android:fontFamily="@font/baloo"
            android:textColor="@color/black"
            android:text="Profile" />

        <Button
            android:id="@+id/button5"
            android:layout_width="match_parent"
            android:layout_margin="20dp"
            android:background="@drawable/logout"
            android:backgroundTint="#FF0000"
            android:fontFamily="@font/baloo"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            android:text="logout" />

        <LinearLayout
            android:layout_width="match_parent"
            android:padding="15dp"
            android:layout_margin="15dp"
            android:elevation="20dp"
            android:layout_height="wrap_content"
            android:background="@drawable/logba"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="28dp"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_plagiarism_24" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="286dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"

                    android:gravity="center_vertical"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:paddingLeft="8dp"
                    android:text="About Us"
                    android:textSize="18dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="wrap_content"
                    android:layout_height="28dp"
                    android:layout_weight="1"
                    android:src="@drawable/cell" />

                <TextView
                    android:id="@+id/textView143"
                    android:layout_width="611dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingLeft="8dp"
                    android:text="Contact Us"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView4qw"
                    android:layout_width="wrap_content"
                    android:layout_height="28dp"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_privacy_tip_24" />

                <TextView
                    android:id="@+id/textView14re3"
                    android:layout_width="286dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:paddingLeft="8dp"
                    android:text="Privecy Policy"
                    android:textSize="18dp" />
            </LinearLayout>


        </LinearLayout>




        <LinearLayout
            android:layout_width="match_parent"
            android:padding="15dp"
            android:layout_margin="15dp"
            android:elevation="20dp"
            android:layout_height="wrap_content"
            android:background="@drawable/logba"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView112"
                    android:layout_width="286dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"

                    android:gravity="center_vertical"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:paddingLeft="8dp"
                    android:text="Connect with Google Fit"
                    android:textSize="18dp" />

                <Switch
                    android:id="@+id/switch1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView1413"
                    android:layout_width="283dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingLeft="8dp"
                    android:text="Connect With Noise"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <Switch
                    android:id="@+id/switch5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="horizontal"/>


        </LinearLayout>

    </LinearLayout>
</FrameLayout>